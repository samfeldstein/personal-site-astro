---
import BaseLayout from "./BaseLayout.astro";

const { frontmatter, headings } = Astro.props;
---

<BaseLayout
  title={frontmatter.key || frontmatter.title}
  description={frontmatter.description} updated={frontmatter.updated} toc={frontmatter.toc} headings={headings}
>
    <slot />
</BaseLayout>
